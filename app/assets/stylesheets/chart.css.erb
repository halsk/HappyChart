/*
* chart.css
*/

.chart {
 position: relative;
 width: 320px;
 height: 320px;
}

div.overlay {
 position: absolute;
 top: 0px;
 left: 0px;
 width: 320px;
 height: 320px;
}

.town {
  padding-left: 24px;
  background-image: url(<%= asset_path 'icon_town.png' %>);
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 20px 22px;
}
.city {
  padding-left: 24px;
  background-image: url(<%= asset_path 'icon_city.png' %>);
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 20px 22px;
}
